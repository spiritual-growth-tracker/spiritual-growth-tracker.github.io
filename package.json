{
  "name": "spiritual-growth-tracker",
  "version": "1.0.0",
  "description": "A web app to track daily spiritual growth by monitoring Fruits of the Spirit and Works of the Flesh",
  "main": "index.js",
  "scripts": {
    "start": "craco start",
    "build": "echo 'Starting build process...' && ls -la public/ && BUILD_PATH=dist craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "clean": "rimraf dist",
    "clean:build": "npm run clean && npm run build",
    "generate-icons": "node generate-favicon.js",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "eslint . --ext .js,.jsx --fix"
  },
  "keywords": [
    "spiritual",
    "growth",
    "tracker",
    "pwa",
    "bootstrap",
    "chart.js"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^5.3.0",
    "bootstrap-icons": "^1.10.0",
    "chart.js": "^4.3.0",
    "flatpickr": "^4.6.13",
    "jquery": "^3.7.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.10",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-flatpickr": "^4.0.10",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.0",
    "@babel/preset-react": "^7.22.0",
    "@craco/craco": "^7.1.0",
    "@eslint/js": "^9.29.0",
    "autoprefixer": "^10.4.14",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "git-rev-sync": "^3.0.2",
    "globals": "^16.2.0",
    "png-to-ico": "^2.1.8",
    "postcss": "^8.4.24",
    "prettier": "^3.5.3",
    "sharp": "^0.34.2",
    "terser-webpack-plugin": "^5.3.7"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "https://spiritual-growth-tracker.github.io/spiritual-growth-tracker.github.io"
}
